# Setup project dependencies
setup:
    #!/bin/bash
    set -e
    
    echo "ðŸ” Setting up project dependencies..."
    
    # Install SQLC
    echo "ðŸ”§ Processing sqlc..."
    if ! command -v sqlc &> /dev/null; then
        echo "ðŸ“¦ Installing sqlc..."
        brew install sqlc
    else
        echo "âœ… sqlc is already installed"
    fi
    
    # Install Goose
    echo "ðŸ”§ Processing goose..."
    if ! command -v goose &> /dev/null; then
        echo "ðŸ“¦ Installing goose..."
        brew install goose
    else
        echo "âœ… goose is already installed"
    fi

    
    echo "ðŸŽ‰ Project setup complete!"
